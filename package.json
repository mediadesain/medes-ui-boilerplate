{
  "name": "medes-ui-boilerplate",
  "version": "1.17.0-angular17",
  "scripts": {
    "ng": "ng",
    "start": "ng lint medes-ui-boilerplate && ng serve medes-ui-boilerplate --port 4600",
    "build": "ng build medes-ui-boilerplate --configuration production",
    "build:lib": "rm -rf node_modules/medes-ui && ng build medes-ui --configuration production && cp -R dist/medes-ui node_modules/",
    "publish:lib": "ng build medes-ui --configuration production && cd dist/medes-ui && npm publish --tag latest",
    "publishbeta:lib": "ng build medes-ui --configuration production && cd dist/medes-ui && npm publish --tag beta",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "medes-ui": "1.17.0-angular17",
    "prismjs": "^1.29.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/builder": "^16.0.0",
    "@angular-eslint/eslint-plugin": "^16.0.0",
    "@angular-eslint/eslint-plugin-template": "^16.0.0",
    "@angular-eslint/schematics": "^16.0.0",
    "@angular-eslint/template-parser": "^16.0.0",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^17.3.0",
    "ts-node": "~8.3.0",
    "typescript": "~5.4.5"
  }
}