{
  "name": "medes-ui-boilerplate",
  "version": "1.15.0",
  "scripts": {
    "ng": "ng",
    "start": "ng lint && ng serve --port 4600",
    "build": "ng build",
    "build:doc": "ng build --configuration production",
    "build:lib": "rm -rf node_modules/medes-ui && ng build medes-ui --configuration production && cp -R dist/medes-ui node_modules/",
    "publish:lib": "ng build medes-ui --configuration production && cd dist/medes-ui && npm publish --tag latest",
    "publishbeta:lib": "ng build medes-ui --configuration production && cd dist/medes-ui && npm publish --tag beta",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "medes-ui": "^1.15.0",
    "prismjs": "^1.29.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^15.2.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  }
}
